@model IEnumerable<ProductCategory>
@foreach (var o in Model)
{
    <tr data-val="@o.Id" class="awe-li" id="productCategory@(o.Id)">
        <td>@o.Description
        </td>
       
        <td>
            <button class="awe-btn" onclick="@(Url.Awe().PopupFormAction().Url(Url.Action("Edit", "ProductCategory", new { o.Id })).Success("edit").Height(300))">
                <span class="ico-edit"></span>
            </button>
        </td>
        <td>
@Html.Partial("deletebtn", new DelBtn{ Id = o.Id, IsDeleted = o.IsDeleted, Controller = "ProductCategory" })
        </td>
    </tr>               
}
