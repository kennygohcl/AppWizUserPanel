           $1$ <button class="awe-btn" onclick="@(Url.Awe().PopupFormAction().Url(Url.Action("Edit", "Lead", new { o.Id })).Success("edit").Height(300))">
                <span class="ico-edit"></span>
            </button>#1#
        </td>
        <td>
          $1$  @Html.Partial("deletebtn", new DelBtn{ Id = o.Id, IsDeleted = o.IsDeleted, Controller = "Lead" })#1#
        </td>*@
