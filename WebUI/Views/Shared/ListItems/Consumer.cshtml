@model IEnumerable<Consumer>
@foreach (var o in Model)
{
    <tr data-val="@o.Id" class="awe-li" id="consumer@(o.Id)">
        <td>@o.Name
        </td>
        <td>@o.Age 
        </td>
        <td>@o.BirthDate 
        </td>
        <td>@o.Gender 
        </td>
        <td>@o.Email 
        </td>
        <td>@o.Phone
        </td>
        <td>@o.Address
        </td>
        <td>@o.DateRegistered
        </td>
